x;\n  padding: 0;\n  margin: 0;\n}\nhtml {\n  font-family: \"Comfortaa\", cursive;\n}\na {\n  text-decoration: none;\n}\n\nbody {\n  position: relative;\n}\n\nnav {\n  height: 56px;\n  padding-top: 0.25em;\n  .nav-wrapper {\n    height: 100%;\n    display: flex;\n  }\n\n  .brand-logo {\n    display: flex;\n    height: 100%;\n    font-weight: 700;\n    color: #000000;\n    font-size: 22px;\n    align-items: center;\n    cursor: pointer;\n    img {\n      height: 100%;\n    }\n    span {\n      width: 150px;\n      display: inline-block;\n      padding-left: 8px;\n      margin-top: 8px;\n    }\n  }\n  .flex-filler {\n    flex-basis: 100%;\n  }\n  .menu-button {\n    height: 100%;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    margin-right: 16px;\n    div {\n      width: 36px;\n      height: 4px;\n      margin: 0 4px 6px;\n      border-radius: 4px;\n      background-color: #263238;\n    }\n  }\n}\n\n.stories-container {\n  width: 90%;\n  margin: 16px auto;\n}\n\n.top-stories-title,\n.user-stories-title {\n  font-size: 28px;\n  font-weight: 700;\n  color: #263238;\n  text-decoration: underline;\n}\n\n.story-container {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 36px;\n  margin-top: 36px;\n  color: #000000;\n  cursor: pointer;\n  div {\n    display: inline-block;\n  }\n\n  .story-desc {\n    display: inline-flex;\n    flex-direction: column;\n    justify-content: space-between;\n    width: 70%;\n    height: 100px;\n    padding: 8px;\n  }\n  .story-title {\n    margin-top: 8px;\n    margin-bottom: 8px;\n    display: block;\n    font-size: 18px;\n    font-weight: 700;\n    line-height: 1.2em;\n  }\n  .story-img {\n    height: 100px;\n    width: 30%;\n    border-radius: 20%;\n    overflow: hidden;\n    img {\n      height: 100%;\n      border-radius: 20%;\n    }\n  }\n  .story-category {\n    padding: 4px 6px;\n    font-weight: 700;\n    color: #dd2c00;\n    background-color: #fbe9e7;\n    border-radius: 4px;\n  }\n  .story-pub-date {\n    padding: 4px 8px;\n  }\n  &.top-story {\n    flex-direction: column-reverse;\n    .story-desc {\n      width: 100%;\n    }\n    .story-img {\n      width: 100%;\n      height: 200px;\n      border-radius: 8px;\n      img {\n        width: 100%;\n        margin: 0 auto;\n        display: block;\n        border-radius: 8px;\n      }\n    }\n  }\n}\n\n.container-back {\n  display: block;\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 100%;\n  background-color: #00000038;\n  z-index: 3000;\n}\n.full-story-container {\n  width: 95%;\n  margin-left: auto;\n  margin-right: auto;\n  margin-top: 48px;\n  padding: 8px;\n  background-color: #ffffff;\n  border-radius: 8px;\n  box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14),\n    0 3px 1px -2px rgba(0, 0, 0, 0.12), 0 1px 5px 0 rgba(0, 0, 0, 0.2);\n\n  .action-btn {\n    position: absolute;\n    top: 0;\n    right: 0;\n    padding: 8px;\n    margin-top: 32px;\n    margin-right: 32px;\n    height: 36px;\n    width: 36px;\n    font-size: 18px;\n    font-weight: 200;\n    background-color: #ffffff;\n    border: none;\n    border-radius: 50%;\n    color: #dd2c00;\n    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14),\n      0 3px 1px -2px rgba(0, 0, 0, 0.12), 0 1px 5px 0 rgba(0, 0, 0, 0.2);\n    &:focus {\n      outline: none;\n    }\n    &:hover {\n      color: #dd2c00;\n    }\n  }\n  .story-title {\n    margin: 16px;\n    display: block;\n    font-size: 24px;\n    font-weight: 700;\n    line-height: 1.2em;\n  }\n  .story-img {\n    height: 300px;\n    width: 100%;\n    border-radius: 4px;\n    overflow: hidden;\n    img {\n      height: 100%;\n      width: 100%;\n      border-radius: 4px;\n    }\n  }\n  .story-content {\n    padding: 8px;\n    padding-top: 16px;\n    font-size: 20px;\n    line-height: 24px;\n    font-weight: 300;\n  }\n  .story-desc {\n    margin: 16px;\n    div {\n      display: inline-block;\n      padding: 4px 8px;\n      text-transform: capitalize;\n      &:first-of-type {\n        font-weight: 700;\n        color: #dd2c00;\n        background-color: #fbe9e7;\n        border-radius: 4px;\n      }\n    }\n  }\n}\n.toast {\n  display: none;\n  position: fixed;\n  top: 60px;\n  left: 0;\n  right: 0;\n  width: fit-content;\n  margin: 0 auto;\n  padding: 8px;\n  font-weight: 500;\n  border-radius: 4px;\n  color: #dd2c00;\n  background-color: #ffffff;\n  box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14),\n    0 3px 1px -2px rgba(0, 0, 0, 0.12), 0 1px 5px 0 rgba(0, 0, 0, 0.2);\n  z-index: 1000;\n}\n\n#installBanner {\n  display: none;\n  position: absolute;\n  top: 400px;\n  left: 0;\n  right: 0;\n  width: 90%;\n  margin: 0 auto;\n  padding: 8px;\n  background-color: #ffb847;\n  border-radius: 8px;\n  box-shadow: 0 4px 5px 0 rgba(0, 0, 0, 0.14), 0 1px 10px 0 rgba(0, 0, 0, 0.12),\n    0 2px 4px -1px rgba(0, 0, 0, 0.3);\n  .banner-header {\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center;\n  }\n  .brand-logo {\n    height: 100px;\n    font-weight: 700;\n    color: #000000;\n    font-size: 22px;\n    align-items: center;\n    cursor: pointer;\n    img {\n      height: 100%;\n    }\n  }\n  .tagline {\n    display: inline-block;\n    padding: 8px;\n    font-size: 20px;\n    font-weight: 500;\n    line-height: 24px;\n  }\n  .user-choice {\n    display: flex;\n    flex-direction: row;\n    justify-content: flex-end;\n  }\n  button {\n    padding: 8px 16px;\n    margin: 8px;\n    border: none;\n    border-radius: 8px;\n    min-height: 36px;\n    color: #000000;\n    font-size: 14px;\n    font-weight: 500;\n    -webkit-appearance: none;\n    background-color: transparent;\n    text-transform: uppercase;\n    &#installBtn {\n      background-color: #ffffff;\n    }\n    &#cancel {\n      border: 1px solid #ffffff;\n      background: none;\n      font-weight: 400;\n    }\n  }\n}\n\n@media only screen and (max-width: 992px) {\n  nav {\n    .brand-logo {\n      display: flex;\n      padding-left: 16px;\n    }\n  }\n}\n\n@media only screen and (min-width: 992px) {\n  .app-container {\n    width: 90%;\n    margin: 0 auto;\n  }\n  .story-container {\n    width: 30%;\n    margin-left: 16px;\n    margin-right: 16px;\n  }\n  .stories-group {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n  }\n}\n\nfooter {\n  padding: 16px 16px 8px 16px;\n  .copyright {\n    font-size: 1.4em;\n    font-weight: 700;\n    text-align: center;\n  }\n  .disclaimer {\n    text-align: center;\n    color: #90a4ae;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"a49f185f007fac16e935e2897d02fa2d","cacheData":{"env":{}}}